<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_pressed="true" android:drawable="@drawable/ic_stylize_face" /> <!--currently pressed turning the toggle on-->
    <item android:state_pressed="true" android:drawable="@drawable/ic_stylize_all" /> <!--currently pressed turning the toggle off-->
    <item android:state_checked="true" android:drawable="@drawable/ic_stylize_all" /> <!--not pressed default checked state-->
    <item android:drawable="@drawable/ic_stylize_all" /> <!--default non-pressed non-checked-->
</selector>